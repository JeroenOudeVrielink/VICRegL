    "version": "0.2.0",
    "configurations": [
        {
            "name": "main",
            "type": "python",
            "request": "launch",
            "program": "main_vicregl.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--fp16",
                "--exp-dir",
                "debug",
                "--arch",
                "resnet50",
                "--epochs",
                "100",
                "--batch-size",
                "4",
                "--optimizer",
                "lars",
                "--base-lr",
                "0.3",
                "--weight-decay",
                "1e-06",
                "--size-crops",
                "224",
                "--num-crops",
                "2",
                "--min_scale_crops",
                "0.08",
                "--max_scale_crops",
                "1.0",
                "--alpha",
                "0.75",
                "--data_path",
                "~/data_hdd/AIML_rot_corrected",
                "--annotations_file",
                "annotations/img_paths_mini.csv",
                "--exp_name",
                "debug"
            ]
        }
    ]
}